#last_session_dialog
    padding 10px 20px
    width fit-content//400px

    & header
        text-align center
        font-size 20px

    & main
        display flex
        flex-direction row
        gap 20px
        & .text_column
            padding-right 0
        & .key
            width 80px
            user-select text
        & .value
            max-width 210px
            max-height 54px
            overflow hidden

    & footer
        margin-top 20px
        text-align center


.app_confirm_dialog_buttons_container
    display flex
    flex-direction row
    margin-top 20px
    gap 10px

#last_session_dialog_button_OK
    width 100px